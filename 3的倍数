#include<iostream>
using namespace std;

int main()
{
	int a, n,m=0,z=0,q=0;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		cin >> a;
		if (a % 3 == 0)
		{
			m++;
		}
		else if (a % 3 == 1)
		{
			z++; 
		}
		else if (a%3==2)
		{
			q++;
		}

	}
	if ((m >= 2) || ((z >= 1)&&(q>=1)))
	{
		cout << "Yes";
	}
	else
	{
		cout << "No";
	}
	
	return 0;
}
