#include<iostream>
using namespace std;
#include<cmath>
int main()
{
	int n, m;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		cin >> m;
		if (m < 2)
		{
			cout << 0 << endl;
		}
		for (int i = 2; i <= m; i++)
		{
			if (m == i)
			{
				cout << 1 << endl;
				break;
			}
			if (m % i == 0)
			{
				cout << 0 << endl;
				break;
			}
		}
		
	}
}
