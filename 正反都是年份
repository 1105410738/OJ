
#include<iostream>;
using namespace std;
int main()
{
	int y, x = 0, m = 0;
	cin >> y;
	int mouth = ((y % 10000) / 100);//月份
	int day = ((y % 10000) % 100);//日
	int year = (y / 10000);//年份

	if (mouth == 2)//2月份
	{
		if (((year % 4 == 0) && (year % 100 != 0) )|| (year % 400 == 0) || (year % 3200) == 0)
		{
			if (day <= 29)
			{
				m++;
			}
		}
		else{
			if (day <= 28)
			{
				m++;
			}
		}
	}
	else if ((mouth == 1) || (mouth == 3) || (mouth == 5) || (mouth == 7) || (mouth == 8) || (mouth == 10) || (mouth == 12))//判断大月
	{
		if (day < 32)
		{
			m++;
		}
	}
	else if ((mouth == 4) || (mouth == 6) || (mouth == 9) || (mouth == 11))//判断小月
	{
		if ( (day < 31))
		{
			m++;
		}
	}
	for (; y != 0;)
	{
		x = y % 10 + x * 10;
		y = y / 10;
	}
	int mouth1 = ((x % 10000) / 100);//月份
	int day1 = ((x % 10000) % 100);//日
	int year1 = (x / 10000);//年份
	
	if (m == 1)//第一次成功
	{
			if (mouth1 == 2)
			{
				if ((year1 % 4 == 0) && (year1 % 100 != 0) || (year1 % 400 == 0) || (year1 % 3200) == 0)
				{
					if (day1 <= 29)
					{
						cout << "Yes";
					}
					else{ cout << "No"; }//日不合格
				}
				else{
					if (day1 <= 28)
					{
						cout << "Yes";
					}
					else{
						cout << "No";//日不合格
					}
				}
			}
			else if ((mouth1 == 1) || (mouth1 == 3) || (mouth1 == 5) || (mouth1 == 7) || (mouth1 == 8) || (mouth1 == 10) || (mouth1 == 12))//判断大月
			{
				if (day1 < 32)
				{
					cout << "Yes";
				}
				else{
					cout << "No";//日不合格
				}
			}
			else if ((mouth1 == 4) || (mouth1 == 6) || (mouth1 == 9) || (mouth1 == 11))//判断小月
			{
				if (day1 < 31)
				{
					cout << "Yes";
				}
				else{ cout << "No"; }//日不合格
			}
			else{ cout << "No"; }
		
	}
	else{ cout << "No"; }
}
