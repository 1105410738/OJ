#include<iostream>
using namespace std;
int gongyue(int a,int b)//求最大公约数（欧几里德求最大公约数）
{
	int t;
	for (; b > 0;)
	{
		t = a;
		a = b;
		b = t%a;
	}
	return a;
}
int gongbei(int c, int d)//求最大公倍数
{
	for (int i = 1; i <= c*d; i++)
	{
		if ((i%c == 0) && (i%d == 0))
		{
			return i;
		}
	}
}
int main()
{
	int n, a, b;
	cin >> n;
	for (int i = 1; i <= n; i++)
	{
		cin >> a >> b;
		cout << gongyue(a, b) <<" "<< gongbei(a, b) << endl;

	}
	
}
	
